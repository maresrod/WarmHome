<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <size
                android:width="40dp"
                android:height="40dp" />
            <solid android:color="#F86F05" />
            <corners
                android:radius="18dp"
                />
        </shape>

    </item>

    <item android:top="70dp">
        <shape android:shape="rectangle">
            <size
                android:width="30dp"
                android:height="30dp" />
            <solid android:color="#FFFFFF" />
            <corners
                android:radius="0dp"
                />
        </shape>

    </item>



</layer-list>
